<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="CoffeDate.Model.Models" assembly="CoffeDate.Model">
  <class name="User" table="Users">
    <id name="UserId" type="int" column="UserId">
      <generator class="native" />
    </id>
    <property name="FirstName" type="string" column="FirstName"/>
    <property name="LastName" type="string" column="LastName"/>
    <property name="EmailAddress" type="string" column="EmailAddress"/>
    <property name="Password" type="string" column="Password"/>
    <property name="CoffeType" type="int" column="CoffeType"/>
    <property name="Sugar" type="int" column="Sugar"/>
    <property name="Additive" type="int" column="Additive"/>
  </class>
</hibernate-mapping>