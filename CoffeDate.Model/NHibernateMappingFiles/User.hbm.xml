<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="CoffeDate.Model.Models" assembly="CoffeDate.Model">
  <class name="User" table="Users">
    <id name="UserId" type="Int32">
      <column name="UserId" sql-type="int" not-null="true" length="4" />
      <generator class="uuid.hex" />
    </id>
    <property name="FirstName" type="String">
      <column name="FirstName" />
    </property>
    <property name="LastName" type="String">
      <column name="LastName" />
    </property>
    <property name="EmailAddress" type="String">
      <column name="EmailAddress" />
    </property>
    <property name="Val" type="Int32">
      <column name="Val" sql-type="int"/>
    </property>
  </class>
</hibernate-mapping>